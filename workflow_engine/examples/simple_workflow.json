{
  "name": "Simple AI Workflow",
  "description": "A simple workflow that uses AI to process text",
  "nodes": [
    {
      "id": "trigger_1",
      "name": "Manual Trigger",
      "type": "trigger",
      "parameters": {
        "trigger_type": "manual"
      }
    },
    {
      "id": "ai_1",
      "name": "AI Processing",
      "type": "ai",
      "parameters": {
        "provider": "openrouter",
        "model": "openai/gpt-3.5-turbo",
        "prompt": "Summarize the following text in one sentence: {{$json.text}}"
      }
    },
    {
      "id": "transform_1",
      "name": "Format Response",
      "type": "transform",
      "parameters": {
        "operation": "format_string",
        "template": "Summary: {response}"
      }
    }
  ],
  "connections": {
    "trigger_1": ["ai_1"],
    "ai_1": ["transform_1"]
  }
}
